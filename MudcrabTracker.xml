<GuiXml>
  <Controls>
    <TopLevelControl name="MudcrabTrackerIndicator" mouseEnabled="true" movable="true" clampedToScreen="false">
      <Dimensions x="275" y="98" />
      <Anchor point="TOPLEFT" relativeTo="GuiRoot" relativePoint="TOPLEFT" />
      <OnMoveStop>
        MudcrabTracker_OnCounterMoveStop()
      </OnMoveStop>
      <Controls>
        <Label name="$(parent)Label" width="275" height="24" font="ZoFontGame|20" inheritAlpha="true" color="FFFFFF"
            wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="CENTER">
          <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
          <Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetX="0" offsetY="0" />
        </Label>
        <Label name="$(parent)RankLabel" width = "275" height="24" font="ZoFontGame|20" inheritAlpha="true" color="FFFFFF"
            wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="CENTER">
          <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="25" />
          <Anchor point="TOPRIGHT" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetX="0" offsetY="25" />
        </Label>
      </Controls>
    </TopLevelControl>
  </Controls>
</GuiXml>
